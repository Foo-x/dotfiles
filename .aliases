# use in your .bash_profile or .bashrc like below
# . /path/to/dotfiles/.aliases

# cd
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias .....='cd ../../../..'
alias doc='cd ~/Documents'
alias dl='cd ~/Downloads'
alias ws='cd ~/Documents/workspace'

alias g=git
alias ll='ls -lah'
alias ls='ls -F --color=auto'
alias vi=vim
