    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    },
    {
      "before": ["<C-a>"],
      "after": ["<HOME>"]
    },
    {
      "before": ["<C-e>"],
      "after": ["<END>"]
    },
    {
