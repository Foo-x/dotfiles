  "markdown.extension.toc.updateOnSave": false,
  "nim.nimsuggestRestartTimeout": 1,
  "sync.gist": "a546a2bbd1739a568924794ad9bc65b6",
  "todohighlight.exclude": [
    "**/node_modules/**",
    "**/bower_components/**",
    "**/dist/**",
    "**/build/**",
    "**/.vscode/**",
    "**/.github/**",
    "**/_output/**",
    "**/*.min.*",
    "**/*.map",
    "**/.next/**",
    "**/deps/**",
    "**/_build/**"
  ],
  "todohighlight.include": [
    "**/*.js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx",
    "**/*.html",
    "**/*.php",
    "**/*.css",
    "**/*.scss",
    "**/*.elm",
    "**/*.ex",
    "**/*.eex"
  ],
  "todohighlight.toggleURI": true,
  "vim.easymotion": true,
  "vim.easymotionMarkerBackgroundColor": "#000",
  "vim.easymotionMarkerFontWeight": "bold",
  "vim.easymotionMarkerForegroundColorOneChar": "#f00",
  "vim.easymotionMarkerForegroundColorTwoCharFirst": "#f00",
  "vim.handleKeys": {
    "<C-b>": false,
    "<C-f>": false,
    "<C-k>": false
  },
  "vim.hlsearch": true,
  "vim.leader": "<space>",
  "vim.useSystemClipboard": true,
