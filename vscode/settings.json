{
    "editor.cursorSurroundingLines": 5,
    "editor.cursorSurroundingLinesStyle": "all",
    // @sync os=windows
    "editor.fontFamily": "Consolas,'Courier New'",
    "editor.fontSize": 15,
    "editor.quickSuggestions": {
        "other": false,
        "comments": false,
        "strings": false
    },
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [
        120
    ],
    "editor.scrollBeyondLastLine": false,
    "files.defaultLanguage": "markdown",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/target/**": true
    },
    "terminal.integrated.splitCwd": "workspaceRoot",
    "vim.easymotion": true,
    "vim.easymotionMarkerBackgroundColor": "#000",
    "vim.easymotionMarkerFontWeight": "bold",
    "vim.easymotionMarkerForegroundColorOneChar": "#f00",
    "vim.easymotionMarkerForegroundColorTwoCharFirst": "#f00",
    "vim.handleKeys": {
        "<C-b>": false,
        "<C-f>": false,
        "<C-k>": false
    },
    "vim.hlsearch": true,
    "vim.leader": "<space>",
