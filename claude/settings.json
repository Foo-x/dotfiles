      "Read(*.key)",
      "Read(~/.ssh/**)",
      "Read(~/.aws/**)",
      "Write(env)",
      "Write(.env*)",
      "Write(*.pem)",
      "Write(*.key)",
      "Write(~/.ssh/**)",
      "Write(~/.aws/**)",
      "Edit(env)",
      "Edit(.env*)",
      "Edit(*.pem)",
      "Edit(*.key)",
      "Edit(~/.ssh/**)",
      "Edit(~/.aws/**)"
    ],
    "defaultMode": "acceptEdits"
  },
  "model": "opusplan",
  "hooks": {
    "PermissionRequest": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "push_ai ${TMUX_PANE} \"claude permission request ${CLAUDE_PROJECT_DIR}\""
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "push_ai ${TMUX_PANE} \"claude stop ${CLAUDE_PROJECT_DIR}\""
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "bun x ccusage statusline",
    "padding": 0
  },
  "language": "japanese",
  "promptSuggestionEnabled": false,
  "sandbox": {
    "enabled": true,
    "autoAllowBashIfSandboxed": true,
    "excludedCommands": ["git", "docker"]
  }
}
