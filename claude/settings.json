      "Bash(jj op abandon *)",
      "Bash(jj operation restore *)",
      "Bash(jj op restore *)",
      "Bash(jj operation revert *)",
      "Bash(jj op revert *)",
      "Bash(jj undo *)",
      "Bash(jj redo *)",
