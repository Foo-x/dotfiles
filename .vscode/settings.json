{
    "editor.renderWhitespace": "boundary",
    "editor.scrollBeyondLastLine": false,
    "files.trimTrailingWhitespace": true,
    "editor.rulers": [
        120
    ],
    "workbench.iconTheme": "material-icon-theme",
    "files.insertFinalNewline": true,
    "todohighlight.include": [
        "**/*.js",
        "**/*.jsx",
        "**/*.ts",
        "**/*.tsx",
        "**/*.html",
        "**/*.php",
        "**/*.css",
        "**/*.scss",
        "**/*.elm",
        "**/*.ex",
        "**/*.eex"
    ],
    "todohighlight.exclude": [
        "**/node_modules/**",
        "**/bower_components/**",
        "**/dist/**",
        "**/build/**",
        "**/.vscode/**",
        "**/.github/**",
        "**/_output/**",
        "**/*.min.*",
        "**/*.map",
        "**/.next/**",
        "**/deps/**",
        "**/_build/**"
    ],
    "todohighlight.toggleURI": true,
    "workbench.startupEditor": "newUntitledFile",
    "window.zoomLevel": 1,
    "terminal.integrated.splitCwd": "workspaceRoot",
    "vim.useSystemClipboard": true,
    "eslint.autoFixOnSave": true,
    "eslint.alwaysShowStatus": true,
    "vim.leader": "<space>",
    "vim.hlsearch": true,
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "1"
            ],
            "after": [
                "0"
            ]
        },
        {
            "before": [
                "0"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "9"
            ],
            "after": [
                "$"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "j"
            ],
            "after": [
                "g",
                "j"
            ]
        },
        {
            "before": [
                "k"
            ],
            "after": [
                "g",
                "k"
            ]
        },
        {
            "before": [
                "<CR>"
            ],
            "after": [
                "G"
            ]
        },
        {
            "before": [
                "<BS>"
            ],
            "after": [
                "g",
                "g"
            ]
        },
        {
            "before": [
                "Y"
            ],
            "after": [
                "y",
                "$"
            ]
        },
        {
            "before": [
                "+"
            ],
            "after": [
                "<C-a>"
            ]
        },
        {
            "before": [
                "-"
            ],
            "after": [
                "<C-x>"
            ]
        },
        {
            "before": [
                "<leader>",
                "h"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "<leader>",
                "l"
            ],
            "after": [
                "$"
            ]
        },
        {
            "before": [
                "<leader>",
                "m"
            ],
            "after": [
                "%"
            ]
        },
        {
            "before": [
                "<leader>",
                "/"
            ],
            "after": [
                "*"
            ]
        },
        {
            "before": [
                "<leader>",
                "j"
            ],
            "commands": [
                "scrollPageDown"
            ]
        },
        {
            "before": [
                "<leader>",
                "k"
            ],
            "commands": [
                "scrollPageUp"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "s"
            ],
            "commands": [
                ":split"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "v"
            ],
            "commands": [
                ":vsplit"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "h"
            ],
            "commands": [
                "workbench.action.focusLeftGroup"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "l"
            ],
            "commands": [
                "workbench.action.focusRightGroup"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "j"
            ],
            "commands": [
                "workbench.action.focusBelowGroup"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "k"
            ],
            "commands": [
                "workbench.action.focusAboveGroup"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "H"
            ],
            "commands": [
                "workbench.action.moveEditorToLeftGroup"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "L"
            ],
            "commands": [
                "workbench.action.moveEditorToRightGroup"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "J"
            ],
            "commands": [
                "workbench.action.moveEditorToBelowGroup"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "K"
            ],
            "commands": [
                "workbench.action.moveEditorToAboveGroup"
            ]
        },
        {
            "before": [
                "<leader>",
                "t",
                "t"
            ],
            "commands": [
                ":tabnew"
            ]
        },
        {
            "before": [
                "<leader>",
                "t",
                "h"
            ],
            "after": [
                "g",
                "T"
            ]
        },
        {
            "before": [
                "<leader>",
                "t",
                "j"
            ],
            "after": [
                "g",
                "T"
            ]
        },
        {
            "before": [
                "<leader>",
                "t",
                "k"
            ],
            "after": [
                "g",
                "t"
            ]
        },
        {
            "before": [
                "<leader>",
                "t",
                "l"
            ],
            "after": [
                "g",
                "t"
            ]
        },
        {
            "before": [
                "<leader>",
                "="
            ],
            "commands": [
                "editor.action.formatDocument"
            ]
        },
        {
            "before": [
                "<leader>",
                "s"
            ],
            "after": [
                ":",
                "%",
                "s",
                "/"
            ]
        },
        {
            "before": [
                "x"
            ],
            "after": [
                "\"",
                "_",
                "x"
            ]
        }
    ],
    "vim.insertModeKeyBindingsNonRecursive": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        },
        {
            "before": [
                "<C-e>"
            ],
            "after": [
                "<END>"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<CR>"
            ],
            "after": [
                "G"
            ]
        },
        {
            "before": [
                "<BS>"
            ],
            "after": [
                "g",
                "g"
            ]
        },
        {
            "before": [
                "Y"
            ],
            "after": [
                "y",
                "$"
            ]
        },
        {
            "before": [
                "<leader>",
                "h"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "<leader>",
                "l"
            ],
            "after": [
                "$"
            ]
        },
        {
            "before": [
                "<leader>",
                "m"
            ],
            "after": [
                "%"
            ]
        }
    ]
}
