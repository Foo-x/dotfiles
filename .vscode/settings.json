            "commands": [
                "editor.action.formatDocument"
            ]
        },
        {
            "before": [
                "<leader>",
                "s"
            ],
            "after": [
                ":",
                "%",
                "s",
                "/"
            ]
        },
        {
            "before": [
                "<leader>",
                "y"
            ],
            "after": [
                "g",
                "g",
                "y",
                "G"
            ]
        },
        {
            "before": [
                "<leader>",
                "o"
            ],
            "after": [
                "o",
                "<Esc>"
            ]
        },
        {
            "before": [
                "<leader>",
                "O"
            ],
            "after": [
                "O",
                "<Esc>"
            ]
        },
        {
            "before": [
                "x"
            ],
            "after": [
                "\"",
                "_",
                "x"
            ]
        },
        {
            "before": [
                "X"
            ],
            "after": [
                "\"",
                "_",
                "X"
            ]
        },
        {
            "before": [
                "s"
            ],
            "after": [
                "\"",
